class Start {
    static int counter;
    
    function void display() {
        let counter = 0;
        
        //64 x 23
        do Output.moveCursor(12, 26);
        do Output.printString("Minesweeper");
        do Output.moveCursor(13, 26);
        do Output.printString("Press Enter");
        
        while (~(Keyboard.keyPressed() = 128)) {
            let counter = counter + 1;
        }
                
        return;
    }
    
    function int getSeed() { return counter; }
}