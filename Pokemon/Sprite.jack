	function void drawHPBar(Pokemon pokemon, bool lower)
	{
		var int left, top;
		var int length, width;
		var int row, col;
		var int i, dmg;
		var String temp;
		var String HP, LVL;

		let HP = "HP: ";
		let LVL = ":L ";

		let length = 43;
		let width = 254;
		if (lower)
		{
			let left = 243;
			let top = 139;
			let row = 13;
			let col = 30;
		}
		else
		{
			let left = 15;
			let top = 7;
			let row = 1;
			let col = 2;
		}

		do Screen.setColor(false);
		do Screen.drawRectangle(left, top, left+width,  top+length);

		do Output.moveCursor(row, col);
		do Output.printString(pokemon.getName());
		let temp = pokemon.getName();
		do Output.moveCursor(row, col + temp.length() + 5);
		do Output.printString(LVL);
		do Output.printInt(pokemon.getLevel());
		do Output.moveCursor(row+2, col+2);
		do Output.printString(HP);

		do Screen.setColor(true);
		if (lower)
		{
			do Screen.drawRectangle(left+width-5,top+15,left+width-1,top+length-1);
			do Screen.drawRectangle(left+8,top+length-5,left+width-1,top+length-1);
			do Screen.setColor(false);
			do Screen.drawRectangle(left+width-2,top+length-2,left+width-1,top+length-1);
			do Screen.setColor(true);
			let i = 0;
			while (i < 8)
			{
				do Screen.drawLine(left+i,top+length-1,left+8,top+(length-9)+i);
				let i = i + 1;
			}
						
		}
		else
		{
			do Screen.drawRectangle(left,top+15,left+4,top+length-1);
			do Screen.drawRectangle(left,top+length-5,left+width-9,top+length-1);
			do Screen.setColor(false);
			do Screen.drawRectangle(left,top+length-2,left+1,top+length-1);
			do Screen.setColor(true);
			let i = 0;
			while (i < 8)
			{
				do Screen.drawLine(left+width-9,top+(length-9)+i,left+(width-1)-i,top+length-1);
				let i = i + 1;
			}
		}

		do Screen.drawRectangle(left+50,top+19,left+width-19,top+20);
		do Screen.drawRectangle(left+50,top+34,left+width-19,top+35);
		do Screen.drawRectangle(left+50,top+19,left+51,top+34);
		do Screen.drawRectangle(left+width-19,top+19,left+width-18,top+34);
		do Screen.drawCircle(left+50,top+27,8);
		do Screen.drawCircle(left+width-19,top+27,8);
		do Screen.setColor(false);
		do Screen.drawRectangle(left+52,top+21,left+width-20,top+33);

		do Screen.setColor(true);
		let dmg = (pokemon.getHP()*180)/pokemon.getMax();
		if (dmg > 0)
		{
			do Screen.drawRectangle(left+53,top+22,left+53+dmg,top+32);
		}
		else
		{
			do Screen.setColor(false);
			do Screen.drawRectangle(left+52,top+21,left+width-20,top+33);
		}

		do HP.dispose();
		do LVL.dispose();

		return;
	}

