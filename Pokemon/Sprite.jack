	function void draw (int x, int y, String name, bool owned)
	{
		if (PokeNames = null)
		{
			let PokeNames = Array.new(6);
			let PokeNames[0] = "Alakazam";
			let PokeNames[1] = "Blastoise";
			let PokeNames[2] = "Charizard";
			let PokeNames[3] = "Primeape";
			let PokeNames[4] = "Scyther";
			let PokeNames[5] = "Venusaur";
		}
		
		if (Constants.strCompare(name, PokeNames[0]))
		{
			do Sprite.drawAlakazam(x, y, owned);
		}
		if (Constants.strCompare(name, PokeNames[1]))
		{
			do Sprite.drawBlastoise(x, y, owned);
		}
		if (Constants.strCompare(name, PokeNames[2]))
		{
			do Sprite.drawCharizard(x, y, owned);
		}
		if (Constants.strCompare(name, PokeNames[3]))
		{
			do Sprite.drawPrimeape(x, y, owned);
		}
		if (Constants.strCompare(name, PokeNames[4]))
		{
			do Sprite.drawScyther(x, y, owned);
		}
		if (Constants.strCompare(name, PokeNames[5]))
		{
			do Sprite.drawVenusaur(x, y, owned);
		}
		return;
	}

	function void drawSprite(Array tuples, int left, int top, int size, int length, int width)
	{
		var Tuple temp;
		var int i;
		var int lx, ty, rx, by;
		
		do Screen.setColor(false);
		do Screen.drawRectangle(left, top, left+width-1, top+length-1 );
		do Screen.setColor(true);  

		let i = 0;
		while (i < size)
		{
			let temp = tuples[i];
			let ty = temp.getTL() / width;
			let lx = temp.getTL() - (ty * width);
			let by = temp.getBR() / width;
			let rx = temp.getBR() - (by * width);
			do Screen.drawRectangle(left+lx,top+ty,left+rx,top+by);
			let i = i + 1;
		}
		return;
	}

	function void drawSpriteFlipped(Array tuples, int left, int top, int size, int length, int width)
	{
		var Tuple temp;
		var int i;
		var int lx, ty, rx, by;
		
		do Screen.setColor(false);
		do Screen.drawRectangle(left, top, left+width-1, top+length-1 );
		do Screen.setColor(true);  

		let i = 0;
		while (i < size)
		{
			let temp = tuples[i];
			let ty = temp.getTL() / width;
			let lx = temp.getTL() - (ty * width);
			let by = temp.getBR() / width;
			let rx = temp.getBR() - (by * width);
			do Screen.drawRectangle(left+(width-1)-rx,top+ty,left+(width-1)-lx,top+by);
			let i = i + 1;
		}
		return;
	}

