	function void drawCursor(int direction, int x, int y, bool draw)
	{
		do Screen.setColor(draw);
		if (direction = 0)
		{
			do Screen.drawRectangle(x+1, y+3, x+1, y+4);
			do Screen.drawRectangle(x+2, y+2, x+2, y+5);
			do Screen.drawRectangle(x+3, y+1, x+3, y+6);
			do Screen.drawRectangle(x+4, y, x+6, y+7);
		}
		if (direction = 1)
		{
			do Screen.drawRectangle(x+1, y, x+3, y+7);
			do Screen.drawRectangle(x+4, y+1, x+4, y+6);
			do Screen.drawRectangle(x+5, y+2, x+5, y+5);
			do Screen.drawRectangle(x+6, y+3, x+6, y+4);
		}
		if (direction = 2)
		{
			do Screen.drawRectangle(x, y+1, x+7, y+3);
			do Screen.drawRectangle(x+1, y+4, x+6, y+4);
			do Screen.drawRectangle(x+2, y+5, x+5, y+5);
			do Screen.drawRectangle(x+3, y+6, x+4, y+6);
		}
		return;
	}

