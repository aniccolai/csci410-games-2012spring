class McGuffin {
	field int x, y;
	field boolean getted;

	constructor McGuffin new( int x_to_set, int y_to_set) {
		let x = x_to_set;
		let y = y_to_set;
		let getted = false;
		return this;
	}

	method boolean is_getted() {
		return getted;
	}

	method void getteded() {
		let getted = true;
		return;
	}

	method int get_x(){
		return x;
	}

	method int get_y(){
		return y;
	}

	method void dispose() {
		do Memory.deAlloc(this);
		return;
	}
}